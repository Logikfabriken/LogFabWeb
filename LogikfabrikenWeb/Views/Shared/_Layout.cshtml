        </nav>

    </div>

    @RenderBody()

    <footer class="page-footer teal">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h5 class="white-text">Logikfabriken AB</h5>
                    <p class="grey-text text-lighten-4">Packhusplatsen 2, 411 13 Göteborg</p>
                    <p class="grey-text text-lighten-4">Telefon: +46 70 798 11 93</p>
                    <p class="grey-text text-lighten-4"> E-post daniel.kretz@logikfabriken.se</p>
