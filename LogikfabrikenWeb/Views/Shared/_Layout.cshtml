                    <a href="https://www.linkedin.com/company/logikfabriken-ab"><img src="content/img/logos/In-2C-48px-R.png" /></a>
                    <a href="https://www.instagram.com/logikfabriken/"><img src="content/img/logos/Instagram_App_Large_May2016.png" /></a>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                Copyright <a class="white-text text-lighten-3" href="http://logikfabriken.se">Logikfabriken</a>
            </div>
        </div>
    </footer>


    <!--  Scripts-->
    <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="scripts/materialize/materialize.min.js"></script>
    <script src="scripts/init.js"></script>
    <script src="scripts/parallax.js"></script>

    @RenderSection("scripts", false);
